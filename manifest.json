{
    "manifest_version" : 3,
    "name": "Tabify",
    "version": "1.0",
    "author": "Shatmanyu Gupta",
    "description": "Remove Duplicate Tabs",
    "icons": {
        "16": "images/24.png",
        "24": "images/24.png",
        "32": "images/24.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions":["tabs","chrome://*/*"],
    "background":{
        "service_worker":"background.js"
    }

}
